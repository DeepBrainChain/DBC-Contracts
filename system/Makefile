compile:
	npx hardhat compile

deploy-logic:
	npx hardhat run scripts/deploy-logic.ts --network dbcTestnet

upgrade-logic:
	npx hardhat run scripts/upgrade-logic.ts --network dbcTestnet

verify-logic:
	npx hardhat verify --network dbcTestnet 0x1d553e987eF6A4779B0f5C5eb2c859E948dcF1BB

